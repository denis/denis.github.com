---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>Denis Barushev</title>
    <link href="http://denis.github.com/atom.xml" rel="self" />
    <link href="http://denis.github.com/" />
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://denis.github.com/</id>
    <author>
        <name>Denis Barushev</name>
        <email>barushev@gmail.com</email>
    </author>
    {% for post in site.posts %}
        <entry>
            <title>{{ post.title }}</title>
            <link href="http://denis.github.com{{ post.url }}"/>
            <updated>{{ post.date | date_to_xmlschema }}</updated>
            <id>http://denis.github.com{{ post.id }}</id>
            <content type="html">{{ post.content | xml_escape }}</content>
        </entry>
    {% endfor %}
</feed>
